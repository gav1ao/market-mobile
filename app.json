{
  "expo": {
    "name": "MarketHelper",
    "slug": "market-helper",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.3",
    "orientation": "portrait",
    "primaryColor": "#003B94",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#003B94"
    },
    "androidNavigationBar": {
      "visible": true,
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.gav1ao.markethelper"
    },
    "android": {
      "package": "br.com.gav1ao.markethelper",
      "permissions": [
        "CAMERA"
      ]
    }
  }
}
